{
  "name": "WayFarer",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation service",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run createTables && cross-env NODE_ENV=development npm run superuser &&  cross-env NODE_ENV=development babel-watch server.js",
    "test": "cross-env NODE_ENV=test nyc  --reporter=text mocha --require babel-register server/tests/*.js --exit --timeout 10000",
    "test:watch": "mocha --watch",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage",
    "superuser": "node --require @babel/register server/config/admin.js",
    "createTables": "node --require @babel/register server/config/tableCreator.js",
    "dropTables": "node --require @babel/register server/config/dropTables.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliezerbasubi/WayFarer.git"
  },
  "keywords": [],
  "author": "Eliezer Basubi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliezerbasubi/WayFarer/issues"
  },
  "homepage": "https://github.com/eliezerbasubi/WayFarer#readme",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.5.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.0.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "object.omit": "^3.0.0",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coverage": "^0.4.1",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.17.2",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  }
}
